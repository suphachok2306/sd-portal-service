<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans"    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"    xsi:schemaLocation="http://www.springframework.org/schema/beans    http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">    <bean id="simSun" class="net.sf.jasperreports.engine.fonts.SimpleFontFamily">        <property name="name" value="SimSun"/>        <property name="normal" value="fonts/SimSun.ttf"/>        <property name="pdfEncoding" value="Identity-H"/>        <property name="pdfEmbedded" value="true"/>    </bean>    <bean id="thSarabunPSK" class="net.sf.jasperreports.engine.fonts.SimpleFontFamily">        <property name="name" value="THSarabunPSK"/>        <property name="normal" value="fonts/THSarabunPSK.ttf"/>        <property name="bold" value="fonts/THSarabunPSK Bold.ttf"/>        <property name="italic" value="fonts/THSarabunPSK Italic.ttf"/>        <property name="boldItalic" value="fonts/THSarabunPSK Bold Italic.ttf"/>        <property name="pdfEncoding" value="Identity-H"/>        <property name="pdfEmbedded" value="true"/>    </bean>    <bean id="thSarabunNew" class="net.sf.jasperreports.engine.fonts.SimpleFontFamily">        <property name="name" value="THSarabunNew"/>        <property name="normal" value="fonts/THSarabunNew.ttf"/>        <property name="bold" value="fonts/THSarabunNew Bold.ttf"/>        <property name="italic" value="fonts/THSarabunNew Italic.ttf"/>        <property name="boldItalic" value="fonts/THSarabunNew BoldItalic.ttf"/>        <property name="pdfEncoding" value="Identity-H"/>        <property name="pdfEmbedded" value="true"/>    </bean></beans>